{
    "name": "intprog/feature-flag-bundle",
    "description": "This bundle contains the feature flag bundle to control features beyond deployments.",
    "homepage": "http://www.intense-programming.com",
    "type": "ezplatform-bundle",
    "license": "MIT",
    "archive": {
        "exclude": [
            "tests",
            "phpunit*.xml"
        ]
    },
    "minimum-stability": "beta",
    "authors": [
        {
            "name": "Steve Konrad",
            "email": "skonrad@wingmail.net",
            "homepage": "http://www.intense-programming.com"
        },
        {
            "name": "IntenseProgramming",
            "homepage": "http://www.intense-programming.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-pdo": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ezsystems/ezplatform-kernel": "~1.0",
        "ezsystems/ezplatform-admin-ui": "~2.0",
        "ezsystems/ezplatform-http-cache": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.1"
    },
    "autoload": {
        "psr-4": {
            "IntProg\\FeatureFlagBundle\\": "bundle/"
        }
    }
}
